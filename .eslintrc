{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "parser": "babel-eslint",
  "extends": [
    "@ijsto",
    "eslint:recommended",
    "airbnb",
    "airbnb/hooks",
    "plugin:react/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:prettier/recommended"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error", {}, { "usePrettierrc": true }],
    "arrow-body-style": "off",
    "prefer-arrow-callback": "off"
  }
}
